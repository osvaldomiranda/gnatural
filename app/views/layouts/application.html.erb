<!DOCTYPE html>
<html>
<head>
  <title>Reportes Grupo Natural</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <% if current_user.present? %>
  <% if current_user.role?("ownercenter") %>
 
  <nav class="navbar navbar-inverse navbar-fixed-top">
    <div class="container-fluid">
      <!-- Brand and toggle get grouped for better mobile display -->
      <div class="navbar-header">
        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <!-- <a class="navbar-brand" href="#">Brand</a> -->
      </div>

   
    <!-- Collect the nav links, forms, and other content for toggling -->
    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">

      <ul class="nav navbar-nav">
 

        <li class="dropdown">
          <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Ventas <span class="caret"></span></a>
          <ul class="dropdown-menu">
            <li>
              <%= link_to home_index_path  do %>
                <span class="iconos"><i class="fa fa-user fa-fw"></i></span>Reporte de Ventas
              <% end %>
            </li>           
          </ul>
        </li>

        <li class="dropdown">
          <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Telefonía <span class="caret"></span></a>
          <ul class="dropdown-menu">
            <li>
              <%= link_to telephony_index_path  do %>
                <span class="iconos"><i class="fa fa-user fa-fw"></i></span>Telefonía
              <% end %>
            </li>           
          </ul>
        </li>
        <li class="dropdown">
          <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Agenda <span class="caret"></span></a>
          <ul class="dropdown-menu">
            <li>
              <%= link_to planyo_index_path  do %>
                <span class="iconos"><i class="fa fa-user fa-fw"></i></span>Agenda
              <% end %>
            </li>           
          </ul>
        </li>        
        
        <% if current_user.role?("admin") %>
        <li class="dropdown">
          <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Admin <span class="caret"></span></a>
          <ul class="dropdown-menu">
            <li>
              <%= link_to admin_index_path  do %>
                <span class="iconos"><i class="fa fa-user fa-fw"></i></span>Informes Detalle
              <% end %>
            </li>         
            <li>
              <%= link_to centers_path  do %>
                <span class="iconos"><i class="fa fa-user fa-fw"></i></span>Centros
              <% end %>
            </li>
            <li>
              <%= link_to owners_path  do %>
                <span class="iconos"><i class="fa fa-user fa-fw"></i></span>Propietarios
              <% end %>
            </li>
            <li>
              <%= link_to kinesiologists_path  do %>
                <span class="iconos"><i class="fa fa-user fa-fw"></i></span>Horas Kine
              <% end %>
            </li>   
            <li>
              <%= link_to admin_kine_yield_path  do %>
                <span class="iconos"><i class="fa fa-user fa-fw"></i></span>Productividad Kine
              <% end %>
            </li> 
            <li>
              <%= link_to admin_user_index_path  do %>
                <span class="iconos"><i class="fa fa-user fa-fw"></i></span>Usuarios
              <% end %>
            </li>   

                        
          </ul>
        </li> 
        <% end %>    
      </ul>



      <ul class="nav navbar-nav navbar-right">
        
        <li class="dropdown">
          <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Sesión <span class="caret"></span></a>
          <ul class="dropdown-menu">

            <li>
              <%= link_to edit_user_registration_path do %>
                <span class="iconos"><i class="fa fa-user fa-fw"></i></span>Cambiar Contraseña
              <% end %>
            </li>

            <li>
              <%= link_to  destroy_user_session_path, :method => :delete do %>
                <span class="iconos"><i class="fa fa-user fa-fw"></i></span>Salir
              <% end %>
            </li>


          </ul>
        </li>
      </ul>
    </div><!-- /.navbar-collapse -->

  </div><!-- /.container-fluid -->
</nav>


<% end %>
<% end %>
</br>
</br>
</br>

<% if current_user.role?("ownercenter") %>
  <%= yield %>
<% end %>
</body>
</html>
